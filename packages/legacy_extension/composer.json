{
  "name": "ssch/legacy-extension",
  "version": "1.0.0",
  "type": "typo3-cms-extension",
  "authors": [
    {
      "name": "Sebastian Schreiber",
      "role": "Developer"
    }
  ],
  "license": [
    "GPL-2.0+"
  ],
  "replace": {
    "legacy_extension": "self.version"
  },
  "require": {
    "typo3/cms-core": "*"
  },
  "require-dev": {
    "typo3/testing-framework": "^4.0"
  },
  "autoload": {
    "psr-4": {
      "Ssch\\LegacyExtension\\": "Classes"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ssch\\LegacyExtension\\Tests\\": "Tests"
    }
  }
}
